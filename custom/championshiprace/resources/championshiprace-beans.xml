<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="vehicleData" class="it.cocacola.championshiprace.data.VehicleData">
		<property name="name" type="String"/>
		<property name="type" type="String"/>
		<property name="number" type="Integer"/>
	</bean>

	<bean id="stableData" class="it.cocacola.championshiprace.data.StableData">
		<property name="name" type="String"/>
		<property name="nation" type="String"/>
		<property name="firstVehicle" type="it.cocacola.championshiprace.data.VehicleData"/>
		<property name="secondVehicle" type="it.cocacola.championshiprace.data.VehicleData"/>
	</bean>

	<bean id="driverData" class="it.cocacola.championshiprace.data.DriverData">
		<property name="name" type="String"/>
		<property name="surname" type="String"/>
		<property name="nationality" type="String"/>
		<property name="vehicle" type="it.cocacola.championshiprace.data.VehicleData"/>
	</bean>

	<bean id="placingData" class="it.cocacola.championshiprace.data.PlacingData">
		<property name="position" type="Integer"/>
		<property name="time" type="String"/>
		<property name="driver" type="it.cocacola.championshiprace.data.DriverData" />
	</bean>

	<bean id="granPrixData" class="it.cocacola.championshiprace.data.GranPrixData">
		<property name="name" type="String"/>
		<property name="date" type="java.util.Date"/>
		<property name="nation" type="String" />
		<property name="laps" type="Integer"/>
		<property name="placing" type="java.util.List&lt;it.cocacola.championshiprace.data.PlacingData&gt;" />
	</bean>

	<bean id="driverEntryData" class="it.cocacola.championshiprace.data.DriverEntryData">
		<property name="position" type="Integer"/>
		<property name="driver" type="it.cocacola.championshiprace.data.DriverData" />
		<property name="points" type="Integer"/>
	</bean>

	<bean id="stableEntryData" class="it.cocacola.championshiprace.data.StableEntryData">
		<property name="position" type="Integer"/>
		<property name="stable" type="it.cocacola.championshiprace.data.StableData" />
		<property name="points" type="Integer"/>
	</bean>

	<bean id="driverRankingData" class="it.cocacola.championshiprace.data.DriverRankingData">
		<property name="placing" type="java.util.List&lt;it.cocacola.championshiprace.data.DriverEntryData&gt;" />
	</bean>

	<bean id="stableRankingData" class="it.cocacola.championshiprace.data.StableRankingData">
		<property name="placing" type="java.util.List&lt;it.cocacola.championshiprace.data.StableEntryData&gt;" />
	</bean>

	<bean id="raceChampionshipData" class="it.cocacola.championshiprace.data.RaceChampionshipData">
		<property name="name" type="String" />
		<property name="type" type="String" />
		<property name="stableRanking" type="it.cocacola.championshiprace.data.StableRankingData" />
		<property name="driverRanking" type="it.cocacola.championshiprace.data.DriverRankingData" />
	</bean>

</beans>
